{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/v1/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "account",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/v1/accounts/{everything}",
      "UpstreamHttpMethod": [ "POST", "PUT", "GET" ],
      "SwaggerKey": "accounts"
    },
    {
      "DownstreamPathTemplate": "/api/v1",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "account",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/v1/accounts",
      "UpstreamHttpMethod": [ "POST", "PUT", "GET" ],
      "SwaggerKey": "accounts" 
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost"
  },
  "SwaggerEndPoints": [
    {
      "Key": "accounts",
      "Config": [
        {
          "Name": "Accounts API",
          "Version": "v1",
          "Url":  "http://account/swagger/v1/swagger.json" 
        }
      ] 
    }
  ] 
}
